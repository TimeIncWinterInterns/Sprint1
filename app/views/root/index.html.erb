		<input type="text" id="subscribers" readonly style="border:0; color:rgb(231,76,60); font-weight:bold;">
		<div id="slider-range2"></div>

		<label for = "views"> Views</label>
		<input type="text" id="views" readonly style="border:0; color:rgb(231,76,60); font-weight:bold;">
		<div id="slider-range3"></div>


		<h3 class = "title"> Audience Criteria </h3>
		<label for = "audiencegender"> Audience's Gender </label>
		<select class="form-control" id= "audiencegender">
			<option>Male</option>
			<option>Female</option>
		</select>

		<label for = "audiencelocation"> Audience's Location </label>
		<input type="text" id="audiencelocation" class = "form-control" placeholder="City, State, Zipcode, Country">

		<label for = "audienceage">  Audience's Age</label>
		<input type="text" id="audienceage" readonly style="border:0; color:rgb(231,76,60); font-weight:bold;">
		<div id="slider-range4"></div>
		<div id= "button">

		<input class="btn btn-lg btn-danger col-xs-7 col-sm-7 col-md-7 col-lg-7 col-xs-offset-2 col-sm-offset-2 col-md-offset-2 col-lg-offset-2" type="button" value="Submit">
	</div>
	</div>
</div>

